{
  "name": "{{groupId}}.pbc-{{appName}}-genx",
  "version": "1.1.0",
  "description": "Genesis {{appName}} PBC Genx",
  "engines": {
    "node": ">=20.0.0"
  },
  "genxSeedConfig": {
    "exclude": [
      "README.md"
    ],
    "persistAnswers": true,
    "processFiles": {
      "exclude": [
        ".git",
        ".genx",
        "node_modules",
        ".gradle",
        "gradle"
      ],
      "rename": true,
      "interpolateVariables": {
        "enabled": true,
        "include": [
          "Dockerfile",
          "product-info",
          ".kt",
          ".md"
        ]
      }
    }
  },
  "dependencies": {
    "@genesislcap/seed-utils": "~14.144.0",
    "tslib": "^2.3.1"
  }
}
